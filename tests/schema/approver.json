                        },
                        "verified": {
                          "type": "boolean",
                          "description": "Indicates whether the Report is verified or not.\n"
                        },
                        "verified_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "RFC 3339, section 5.6 \"https://tools.ietf.org/html/rfc3339#section-5.6\" compliant timestamp on which the Report was verified. If the Report is not verified yet, this could be null.\n"
                        },
                        "reimbursed_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "RFC 3339, section 5.6 \"https://tools.ietf.org/html/rfc3339#section-5.6\" compliant timestamp on which the Report was Reimbursed. If the Report is not reimbursed or submitted yet, this could be null.\n"
                        },
                        "settlement_id": {
                          "type": "string",
                          "maxLength": 255,
                          "description": "An unique ID, generated by Fyle, for identifying the Settlement. Once a Report is paid, the value of this field cannot be changed and is immutable. <br>\n"
                        },
                        "settled_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "RFC 3339, section 5.6 \"https://tools.ietf.org/html/rfc3339#section-5.6\" compliant timestamp on which the report was settled. <br>\n"
                        },
                        "num_expenses": {
                          "type": "integer"
                        },
                        "expense_ids": {
                          "type": "array",
                          "items": {
                            "type": "string"
                          }
                        },
                        "verification_state": {
                          "type": "string"
                        },
                        "creator_id": {
                          "type": "string",
                          "maxLength": 255,
                          "description": "An unique ID, generated by Fyle, for identifying the Employee who created the object. Once an object is created, the value of this field cannot be changed and is immutable.<br>\n"
                        },
                        "creator": {
                          "description": "Details of employee who created this report.\n",
                          "type": "object",
                          "properties": {
                            "user_id": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "This id helps to identify employees in other organisations.\nIt is always prefixed with the string `us`.\nIt is unique per email.\n"
                            },
                            "email": {
                              "type": "string",
                              "format": "email",
                              "maxLength": 127,
                              "description": "An immutable field that represents the email address.\nMust be compliant with <a href=\"https://www.ietf.org/rfc/rfc822.txt\">RFC 822.</a>\n"
                            },
                            "code": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "This field refers to any string that the client programs can use to refer to an employee's unique id.\nThis could be any field such as an UUID from an Active Directory server or employee id in a HRMS system.\n"
                            },
                            "location": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "level_id": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "department_id": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "business_unit": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the business unit of the organization to which of the employee belongs."
                            },
                            "title": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the title/desgination of the employee."
                            },
                            "mobile": {
                              "type": "string",
                              "description": "Mobile number of the employee.\n`Given example is a fake number. We don't know who might answer your call.`\n"
                            },
                            "joined_at": {
                              "type": "string",
                              "format": "date",
                              "description": "Date string in YYYY-MM-DD format"
                            },
                            "full_name": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the full name of the employee. Must be at least 3 characters."
                            }
                          }
                        },
                        "trip_request_id": {
                          "type": "string"
                        },
                        "physical_bill": {
                          "type": "boolean"
                        },
                        "physical_bill_at": {
                          "type": "string"
                        },
                        "manual_flag": {
                          "type": "boolean"
                        },
                        "policy_flag": {
                          "type": "boolean"
                        },
                        "type": {
                          "type": "string"
                        },
                        "started_at": {
                          "type": "string"
                        },
                        "ended_at": {
                          "type": "string"
                        },
                        "locations": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "properties": {
                              "city": {
                                "type": "string"
                              },
                              "state": {
                                "type": "string"
                              },
                              "display_name": {
                                "type": "string"
                              },
                              "country": {
                                "type": "string"
                              },
                              "formatted_address": {
                                "type": "string"
                              },
                              "latitude": {
                                "type": "number"
                              },
                              "longitude": {
                                "type": "number"
                              }
                            }
                          }
                        },
                        "report_approvals": {
                          "type": "array",
                          "items": {
                            "type": "object",
                            "description": "siginifies all the approvals of a particular object.\n",
                            "properties": {
                              "approver_id": {
                                "type": "string"
                              },
                              "state": {
                                "type": "string",
                                "enum": [
                                  "APPROVED",
                                  "APPROVAL_PENDING",
                                  "APPROVAL_DISABLED"
                                ]
                              },
                              "rank": {
                                "type": "integer"
                              }
                            }
                          }
                        },
                        "org_id": {
                          "type": "string",
                          "description": "The unique id of the Organization to which the object is associated.\n"
                        }
                      }
                    }
                  }
                }
              }
            }
          },
          "$schema": "http://json-schema.org/draft-04/schema#"
        },
