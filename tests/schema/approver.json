{
  "/approver/report_approvals": {
    "post": {
      "body": {
        "type": "object",
        "properties": {
          "data": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "description": "Id of a report which is to be approved.   \n"
                }
              }
            }
          }
        },
        "required": [
          "data"
        ],
        "$schema": "http://json-schema.org/draft-04/schema#"
      },
      "responses": {
        "200": {
          "type": "object",
          "properties": {
            "url": {
              "type": "string"
            },
            "data": {
              "type": "array",
              "items": {
                "type": "object",
                "required": [
                  "data"
                ],
                "properties": {
                  "count": {
                    "description": "The total number of objects that can be requested.\n",
                    "type": "integer"
                  },
                  "url": {
                    "type": "string"
                  },
                  "data": {
                    "type": "array",
                    "items": {
                      "type": "object",
                      "properties": {
                        "id": {
                          "type": "string",
                          "maxLength": 255,
                          "description": "This id prodvided by Fyle to uniquely identify a Expense report.\nThis id is always prefixed with `rp`.\n"
                        },
                        "employee_id": {
                          "type": "string",
                          "maxLength": 255,
                          "description": "This id prodvided by Fyle to uniquely identify an employee within an organization.\nThis id is always prefixed with `ou`.\n"
                        },
                        "employee": {
                          "type": "object",
                          "description": "Details of the employee to whom this object belongs to.",
                          "properties": {
                            "user_id": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "This id helps to identify employees in other organisations.\nIt is always prefixed with the string `us`.\nIt is unique per email.\n"
                            },
                            "email": {
                              "type": "string",
                              "format": "email",
                              "maxLength": 127,
                              "description": "An immutable field that represents the email address.\nMust be compliant with <a href=\"https://www.ietf.org/rfc/rfc822.txt\">RFC 822.</a>\n"
                            },
                            "code": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "This field refers to any string that the client programs can use to refer to an employee's unique id.\nThis could be any field such as an UUID from an Active Directory server or employee id in a HRMS system.\n"
                            },
                            "location": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "level_id": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "department_id": {
                              "type": "string",
                              "maxLength": 255
                            },
                            "business_unit": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the business unit of the organization to which of the employee belongs."
                            },
                            "title": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the title/desgination of the employee."
                            },
                            "mobile": {
                              "type": "string",
                              "description": "Mobile number of the employee.\n`Given example is a fake number. We don't know who might answer your call.`\n"
                            },
                            "joined_at": {
                              "type": "string",
                              "format": "date",
                              "description": "Date string in YYYY-MM-DD format"
                            },
                            "full_name": {
                              "type": "string",
                              "maxLength": 255,
                              "description": "Represents the full name of the employee. Must be at least 3 characters."
                            }
                          }
                        },
                        "created_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "<a href='https://tools.ietf.org/html/rfc3339#section-5.6'>RFC 3339, section 5.6 compliant timestamp.</a>\nSignifies when this object was created.\n"
                        },
                        "updated_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "<a href='https://tools.ietf.org/html/rfc3339#section-5.6'>RFC 3339, section 5.6 compliant timestamp.</a>\nSignifies when this object was updated.\n"
                        },
                        "submitted_at": {
                          "type": "string",
                          "format": "date-time",
                          "description": "RFC 3339, section 5.6 \"https://tools.ietf.org/html/rfc3339#section-5.6\" compliant timestamp on which the Report was submitted to the Approvers.\nIf the Report is not yet submitted, this could be null. <br>\n"
